#include <stdlib.h>
#include <stdio.h>

int main()
{
    int valor1,valor2,valor3;
    printf("Digite o primeiro valor =");
    scanf("%d",&valor1);
    printf("Digite o segundo valor =");
    scanf("%d",&valor2);
    printf("VocÃª digitou valor1=%d e valor2=%d ",valor1,valor2);
    valor3=valor1;
    valor1=valor2;
    valor2=valor3;
    printf("\nFicou valor1=%d e valor2=%d ",valor1,valor2);
    system("pause");
    return 0;
}
